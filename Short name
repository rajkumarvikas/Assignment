import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class NameSorter {
    public static void main(String[] args) throws IOException {
        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));

        // Create an array to store the names
        String[] names = new String[10];

        // Read 10 names from the user
        for (int i = 0; i < 10; i++) {
            System.out.print("Enter name #" + (i + 1) + ": ");
            names[i] = reader.readLine();
        }

        // Perform bubble sort to sort the names
        for (int i = 0; i < 10 - 1; i++) {
            for (int j = 0; j < 10 - i - 1; j++) {
                // Compare adjacent names and swap if necessary
                if (compareNames(names[j], names[j + 1]) > 0) {
                    String temp = names[j];
                    names[j] = names[j + 1];
                    names[j + 1] = temp;
                }
            }
        }

        // Print the sorted names
        System.out.println("Sorted Names:");
        for (String name : names) {
            System.out.println(name);
        }
    }

    // Custom method to compare two names
    private static int compareNames(String name1, String name2) {
        // Compare names character by character
        int minLength = Math.min(name1.length(), name2.length());
        for (int i = 0; i < minLength; i++) {
            char char1 = name1.charAt(i);
            char char2 = name2.charAt(i);

            if (char1 != char2) {
                return char1 - char2;
            }
        }

        // If the common characters are equal, compare based on length
        return name1.length() - name2.length();
    }
}
